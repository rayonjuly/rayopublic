    "class Solution:\n",
    "    def coloredCells(self, n: int) -> int:\n",
    "        n = int(n)\n",
    "        in_side = n - 1\n",
    "        out_side = 2*n - 1\n",
    "        sq_outside = (2*n - 1)**2\n",
    "        triangle = n * (n-1) / 2\n",
    "\n",
    "        return int(sq_outside - 4 * triangle)\n",
    "    \n",
    "s = Solution()\n",
    "s.coloredCells(n = 3)"
   ]
